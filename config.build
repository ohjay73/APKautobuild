#!/bin/bash

# Set build environment
# BUILDENV=autotune
# BUILDENV=LH

# Mounting local data on container
data_mount=/data

# Build location under data_mount
publishdir=builds

# Local data
if [ -z ${BUILDENV} ]; then 
  data_local=$(pwd)/data;
else  
  data_local=$(pwd)/data-${BUILDENV};
fi;

image=theod00r/apkbuilder:1.1.9
